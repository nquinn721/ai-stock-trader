# Local Production Environment Variables
# Copy this to .env.local-prod and fill in your actual API keys

# Database Configuration
MYSQL_ROOT_PASSWORD=root_password
MYSQL_DATABASE=trading_app_local_prod
MYSQL_USER=trading_user
MYSQL_PASSWORD=trading_pass
DATABASE_URL=mysql://trading_user:trading_pass@localhost:3307/trading_app_local_prod

# External API Keys (use your actual keys)
# Note: API URLs are now centralized in config files
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here
FINNHUB_API_KEY=your_finnhub_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# LLM Configuration
LLM_PROVIDER=openai  # or 'anthropic'
LLM_MODEL=gpt-4-turbo-preview
LLM_MAX_TOKENS=1000

# Application URLs
REACT_APP_API_URL=http://localhost:8100
REACT_APP_WS_URL=ws://localhost:8100

# Production Settings
NODE_ENV=production
PORT=8000
